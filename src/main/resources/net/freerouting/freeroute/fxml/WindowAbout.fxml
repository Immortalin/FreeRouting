<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.freerouting.freeroute.WindowAboutController">
    <stylesheets>
        <URL value="@/styles/windowabout.css" />
    </stylesheets>
   <children>
      <VBox alignment="CENTER" layoutX="0.0" layoutY="0.0" spacing="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label alignment="CENTER" text="%description">
               <padding>
                  <Insets bottom="10.0" left="5.0" right="10.0" top="10.0" />
               </padding>
            </Label>
            <Label fx:id="version" alignment="CENTER" text="%version">
               <padding>
                  <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
               </padding>
            </Label>
            <Label alignment="CENTER" text="%homepage">
               <padding>
                  <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
               </padding>
            </Label>
            <Label alignment="CENTER" text="%warranty">
               <padding>
                  <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
               </padding>
            </Label>
            <Label alignment="CENTER" text="%support">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>

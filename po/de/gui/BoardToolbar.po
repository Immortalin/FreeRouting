#. # German version of language dependent text of the board toolbar
#: select_button
msgid "Select"
msgstr "Selektieren"

#: select_button_tooltip
msgid "left button selects items (s)"
msgstr "linke Maustaste selektiert Objekte (s)"

#: route_button
msgid "Route"
msgstr "Routen"

#: route_button_tooltip
msgid "left button starts a new trace (r)"
msgstr "linke Maustaste beginnt eine neue Bahn (r)"

#: drag_button
msgid "Drag"
msgstr "Ziehen"

#: drag_button_tooltip
msgid ""
"to drag items with the left button or to push traces out of empty space (d)"
msgstr ""
"Objekte mit gedrückter linker Maustaste ziehen oder Bahnen vom leeren Raum "
"aus schieben(d)"

#: undo_button
msgid "Undo"
msgstr "Undo"

#: undo_button_tooltip
msgid "to undo the last change (u)"
msgstr "die letzte Änderung rückgängig machen (u)"

#: redo_button
msgid "Redo"
msgstr "Redo"

#: redo_button_tooltip
msgid "to redo the last change (b)"
msgstr "die letzte Änderung wiederherstellen (b)"

#: incompletes_button
msgid "Incompletes"
msgstr "Incompletes"

#: incompletes_button_tooltip
msgid "to display or hide the incomplete connections (g)"
msgstr "das Zeigen der offenen Verbindungen an- oder abschalten (g)"

#: violations_button
msgid "Violations"
msgstr "Verletzungen"

#: violations_button_tooltip
msgid "to display or hide the clearance violations (v)"
msgstr "das Zeigen der Clearance-Verletzungen an- oder abschalten (v)"

#: autoroute_button
msgid "Autorouter"
msgstr "Autorouter"

#: autoroute_button_tooltip
msgid "to start the batch autorouter"
msgstr "den Batch-Autorouter starten"

#: display_all_button
msgid "Zoom All"
msgstr "Alles abbilden"

#: display_all_button_tooltip
msgid "to display the whole board (a)"
msgstr "die gesamte Platine abbilden (a)"

#: display_region_button
msgid "Zoom Region"
msgstr "Ausschnitt abbilden"

#: display_region_button_tooltip
msgid "to select a region of the board for display(f)"
msgstr "ein Rechteck der Platine zum Abbilden auswählen (f)"

#: unit_button
msgid "Unit:"
msgstr "Unit:"
